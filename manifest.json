{
  "manifest_version": 2,
  "name": "Interrobanged",
  "description": "A simple script that adds edits interrobangs into your web page",
  "version": "2.0",
  "icons": { "128": "icon-on.png" },
  "browser_action": {
    "default_icon": "icon-on.png",
    "default_title": "Interrobanged"
  },
  "permissions": ["tabs", "http://*/", "https://*/", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
